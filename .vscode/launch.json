  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Tests",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}",
      "env": {
        "GROUP_NAME": "ost.ai",
        "TEST_ZONE_NAME": "ost.ai."
      },
      "trace": "verbose",
      "args": [
        "-test.v",
        "-test.run",
        "TestRunsSuite"
      ],
      "showLog": true,
      // "logOutput": "debugger"
    }
  ]
}
